# Test skipping a test

gno test -v .

stdout 'Hey'
stdout 'I.m on strike!'
! stdout 'this shouldn.t print'
stdout '--- SKIP: TestSkip'
stderr 'ok  '

-- skip.gno --
package skip

-- skip_test.gno --
package skip

import "testing"

func TestSkip(t *testing.T) {
	t.Log("Hey")
	t.Skip("I'm on strike!")
	t.Log("so this shouldn't print")
}
